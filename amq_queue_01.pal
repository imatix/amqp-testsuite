<?xml?>
<pal
    script = "amq_pal_gen"
    desc   = "the server MUST be able to declare a queue"
    >
    This test sets up a session where a default queue is created. A default
    queue is one declared with no arguments.
    
    <include filename = "amq_test_base.pal" />
    <session>
        <queue_declare queue = "myqueue" />
    </session>
</pal>


