<?xml?>
<pal
    script = "amq_pal_gen"
    desc   = "the server MUST respect the prefetch_count on consume"
    >
    This test consumes, with explicit ack, on a queue with prefetch count X.
    2X messages are routed to the queue; we acknowledge none of the received messages,
    therefore we expect half the sent messages still waiting in the queue.

    <include filename = "amq_test_base.pal" />
    <session>
        <set name = "count" value = "0" />
        <queue_declare queue = "myqueue" />
        <basic_consume queue = "myqueue" prefetch_count = "5" no_ack = "0" />
        
        <basic_content />
        <repeat times = "10">
            <basic_publish exchange = "" routing_key = "myqueue" />
        </repeat>
        <wait timeout = "5000" />
        <repeat>
            <basic_arrived>
                <inc name = "count" />
            </basic_arrived>
            <empty>
                <if name = "count" test = "gt" value = "5">
                    <echo>$count</echo>
                </if>
                <assert name = "count" test = "lt" value = "6" />
                <break />
            </empty>
            <wait timeout = "500" />
        </repeat>
    </session>
</pal>


